    "eslint-loader": "^1.8.0",
    "mocha": "^3.4.2",
    "mocha-loader": "^1.1.1",
    "nodemon": "^1.11.0",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.0",
    "talkback": "^1.1.3",
